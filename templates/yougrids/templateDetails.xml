				<field name="insetcolumn_itmid" type="yjsgtext" default="18" label="SPII_IC_LABEL" description="SPII_IC_DESC" />
				<field name="leftcolumn_itmid" type="yjsgtext" default="18" label="SPII_LC_LABEL" description="SPII_RC_LABEL" />
				<field name="rightcolumn_itmid" type="yjsgtext" default="18" label="SPII_RC_LABEL" description="SPII_RC_DESC" />
