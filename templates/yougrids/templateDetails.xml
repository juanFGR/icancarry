				<field name="DGTEXT_LABEL" type="yjsgtextblank" default="DGTEXT_LABEL"/>
				<field name="MAINB_YJ_LABEL" type="yjsgparamtitle" default="MAINB_YJ_LABEL" />
				<field name="css_widthdefined" type="yjsgradio" default="px" label="SITE_WIDTH_DEF_IN_LABEL" description="SITE_WIDTH_DEF_IN_DESC" >
					<option value="%">Percentage</option>
					<option value="px">Pixels</option>
				</field>
				<field name="css_width" type="yjsgtext" default="1200" label="DEF_SITE_WIDTH_LABEL" description="DEF_SITE_WIDTH_DESC" />
				<field name="site_layout" type="yjsglist" default="2" label="DEF_LAYOUT_LABEL" description="DEF_LAYOUT_DESC">
					<option value="1">Leftblock-Midblock-Rightblock</option>
					<option value="2">Midblock-Leftblock-Rightblock</option>
					<option value="3">Leftblock-Rightblock-Midblock</option>
				</field>
				<field name="MBC_W_LABEL" type="yjsgparamtitle" default="MBC_W_LABEL"  />
				<field name="widthdefined" type="yjsgradio" default="%" label="MBC_W_D_I_LABEL" description="MBC_W_D_I_DESC" >
					<option value="%">Percentage</option>
					<option value="px">Pixels</option>
				</field>
				<field name="maincolumn" type="yjsgtext" default="55" label="MBC_LABEL" description="MBC_DESC" />
				<field name="insetcolumn" type="yjsgtext" default="0" label="IC_LABEL" description="IC_DESC" />
				<field name="leftcolumn" type="yjsgtext" default="22.5" label="LC_LABEL" description="LC_DESC" />
				<field name="rightcolumn" type="yjsgtext" default="22.5" label="RC_LABEL" description="RC_DESC" />
				<field name="SPII_LABEL" type="yjsgparamtitle" default="SPII_LABEL" />
