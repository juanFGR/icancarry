			</fieldset>
			<!-- Modules Grids settings -->
			<fieldset name="MG_LABEL">
				<field name="MG_SUB" type="yjsgtextblank" default="MG_SUB"/>
				<field name="MGTEXT_LABEL" type="yjsgtextblank" default="MGTEXT_LABEL"/>
