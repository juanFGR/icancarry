	<config>
		<fields name="params" addfieldpath="/templates/yougrids/elements">
			<fieldset name="YJSG_VERSION_CHECK">
				<field name="" type="yjsgversion" default=""/>
			</fieldset>
			<!-- Style Settings -->
			<fieldset name="STYLE_SETTINGS">
				<field name="STYLE_SETTINGS_TAB" type="yjsgtextblank" default="STYLE_SETTINGS_TAB"/>
				<field name="" type="yjsgtextblank" default="CUSTOM_CSS_SLABEL"/>
				<field name="custom_css" type="yjsgradio" default="2" label="CUSTOM_CSS_LABEL" description="CUSTOM_CSS_DESC">
					<option value="1">Yes</option>
					<option value="2">No</option>
				</field>
				<field name="STTEXT_LABEL" type="yjsgtextblank" default="STTEXT_LABEL"/>
				<field name="STYLE_SUB" type="yjsgparamtitle" default="STYLE_SUB" />
				<field name="yjsg_get_styles" type="yjsglist" default="blue|007ebd" label="COLOR_LABEL" description="COLOR_DESC" yjsgstyles="blue==007ebd|mesh==121212|metal==121212">
					<option value="blue|007ebd">Blue</option>
					<option value="mesh|121212">Mesh</option>
					<option value="metal|121212">Metal</option>
				</field>
				<field name="default_font" type="yjsgtext" default="12px" label="FONT_SIZE_LABEL" description="FONT_SIZE_DESC" />
				<field name="default_font_family" type="yjsglist" default="6" label="FONT_FAM_LABEL" description="FONT_FAM_DESC">
					<option value="1">Bookman Old Style</option>
					<option value="2">Palatino</option>
					<option value="3">Times New Roman</option>
					<option value="4">Century Gothic</option>
					<option value="5">Lucida Grande</option>
					<option value="6">Arial Helvetica</option>
					<option value="7">Tahoma</option>
					<option value="8">Verdana</option>
					<option value="9">Coperplate Gothic</option>
					<option value="10">Lucida Console</option>
					<option value="11">Gill Sans MT</option>
					<option value="12">Trebuchet MS</option>
					<option value="13">Courier New</option>
					<option value="14">Cambria</option>
					<option value="15">Georgia</option>
				</field>
				<field name="selectors_override" type="yjsglist" default="1" label="HTAG_OVR_LABEL" description="HTAG_OVR_DESC">
					<option value="1"  enable="selectors_override_type|css_font_family|google_font_family|jform_params_affected_selectors|jform_params_fontfacekit_font_family">Yes</option>
					<option value="2" disable="selectors_override_type|css_font_family|google_font_family|jform_params_affected_selectors|jform_params_fontfacekit_font_family">No</option>
				</field>
				<field name="selectors_override_type" type="yjsglist" default="3" label="HTAG_OVR_TYPE_LABEL" description="HTAG_OVR_TYPE_DESC">
					<option value="1" enable="css_font_family|jform_params_affected_selectors" disable="google_font_family|jform_params_fontfacekit_font_family">CSS</option>
					<option value="2" enable="google_font_family|jform_params_affected_selectors" disable="css_font_family|jform_params_fontfacekit_font_family">Google Font</option>
					<option value="3" enable="jform_params_fontfacekit_font_family" disable="google_font_family|css_font_family|jform_params_affected_selectors">@font-face kit</option>
				</field>
				<field name="css_font_family" type="yjsglist" default="12" label="CSS_FONT_FAM_LABEL" description="CSS_FONT_FAM_DESC">
					<option value="1">Bookman Old Style</option>
					<option value="2">Palatino</option>
					<option value="3">Times New Roman</option>
					<option value="4">Century Gothic</option>
					<option value="5">Lucida Grande</option>
					<option value="6">Arial Helvetica</option>
					<option value="7">Tahoma</option>
					<option value="8">Verdana</option>
					<option value="9">Coperplate Gothic</option>
					<option value="10">Lucida Console</option>
					<option value="11">Gill Sans MT</option>
					<option value="12">Trebuchet MS</option>
					<option value="13">Courier New</option>
					<option value="14">Cambria</option>
					<option value="15">Georgia</option>
				</field>
				<field name="google_font_family" type="yjsgtext" default="Lobster|sans-serif|normal" label="G_FONT_FAM_LABEL" description="G_FONT_FAM_DESC" size="80" />
				<field name="fontfacekit_font_family" type="folderlist" class="" default="PTSansNarrowBold" hide_default="blank" label="FFK_FONT_FAM_LABEL" directory="/templates/yougrids/css/fontfacekits" description="FFK_FONT_FAM_DESC" />
				<field name="affected_selectors" type="textarea" rows="8" cols="50" default="h1,h2,h3,h4,h5,h6,.article_title,.module_title,.pagetitle" label="AFF_SELECTORS_LABEL" description="AFF_SELECTORS_DESC"/>
